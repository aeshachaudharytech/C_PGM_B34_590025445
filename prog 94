#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    fgets(str, sizeof(str), stdin);

    char longest[1000] = "", word[1000];
    int maxLen = 0;

    int i = 0, j = 0;
    while(1) {
        if(str[i] != ' ' && str[i] != '\0' && str[i] != '\n') {
            word[j++] = str[i];
        } else {
            word[j] = '\0';
            if(j > maxLen) {
                maxLen = j;
                strcpy(longest, word);
            }
            j = 0;
        }
        if(str[i] == '\0' || str[i] == '\n')
            break;
        i++;
    }

    printf("%s", longest);
    return 0;
}
