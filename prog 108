#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int nums[n];
    int result[n];

    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    for(int i = 0; i < n; i++) {
        int product = 1;
        for(int j = 0; j < n; j++) {
            if(i != j) {
                product *= nums[j];
            }
        }
        result[i] = product;
    }

    printf("[");
    for(int i = 0; i < n; i++) {
        printf("%d", result[i]);
        if(i < n - 1)
            printf(",");
    }
    printf("]\n");

    return 0;
}
