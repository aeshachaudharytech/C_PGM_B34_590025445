#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    fgets(str, sizeof(str), stdin);

    int i = 0;
    if(str[i] != ' ' && str[i] != '\n' && str[i] != '\0')
        printf("%c.", str[i] - 32*(str[i]>='a' && str[i]<='z'));
    
    while(str[i] != '\0' && str[i] != '\n') {
        if(str[i] == ' ' && str[i+1] != ' ' && str[i+1] != '\0' && str[i+1] != '\n')
            printf("%c.", str[i+1] - 32*(str[i+1]>='a' && str[i+1]<='z'));
        i++;
    }

    return 0;
}
