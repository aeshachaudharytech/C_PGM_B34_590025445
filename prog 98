#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    fgets(str, sizeof(str), stdin);

    int i = 0, lastSpace = -1;
    while(str[i] != '\0' && str[i] != '\n') {
        if(str[i] == ' ')
            lastSpace = i;
        i++;
    }

    i = 0;
    while(i < lastSpace) {
        if(i == 0 || (str[i-1] == ' ' && str[i] != ' ')) {
            printf("%c.", str[i] - 32*(str[i]>='a' && str[i]<='z'));
        }
        i++;
    }

    if(lastSpace != -1) {
        printf(" ");
        for(i = lastSpace + 1; str[i] != '\0' && str[i] != '\n'; i++)
            printf("%c", str[i]);
    }

    return 0;
}
