#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int maxSoFar = arr[0];
    int currentSum = arr[0];

    for(int i = 1; i < n; i++) {
        // Choose between continuing or starting fresh
        if(currentSum + arr[i] > arr[i])
            currentSum += arr[i];
        else
            currentSum = arr[i];

        // Update maximum
        if(currentSum > maxSoFar)
            maxSoFar = currentSum;
    }

    printf("%d\n", maxSoFar);
    return 0;
}
