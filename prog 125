#include <stdio.h>

int main() {
    char filename[100], text[200];
    FILE *fp;

    scanf("%s", filename);

    fp = fopen(filename, "a");
    if (fp == NULL) return 1;

    getchar(); // consume leftover newline
    fgets(text, sizeof(text), stdin);

    fprintf(fp, "%s", text);
    fclose(fp);

    return 0;
}
